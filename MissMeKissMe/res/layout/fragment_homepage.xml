<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dashboard_items"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:id="@+id/ll_mainnn"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:orientation="vertical" >

        <ScrollView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:scrollbars="none" >

            <LinearLayout
                android:id="@+id/ll_main"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/ll_main_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:orientation="horizontal"
                    android:padding="@dimen/five" >

                    <ImageView
                        android:id="@+id/new_kiss"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/five"
                        android:src="@drawable/new_kiss_icon" />

                    <ImageView
                        android:id="@+id/Blind_kiss"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/five"
                        android:src="@drawable/blind_kiss_icon" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_main_two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:orientation="horizontal"
                    android:padding="@dimen/five" >

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <ImageView
                            android:id="@+id/valut_kiss"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/five"
                            android:src="@drawable/vault_kiss_icon" />

                        <TextView
                            android:id="@+id/valut_kiss_count_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right|top"
                            android:layout_marginRight="5sp"
                            android:background="@drawable/counter_bg"
                            android:gravity="center"
                            android:textColor="#FFF"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:visibility="gone" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <ImageView
                            android:id="@+id/travling_kiss"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/five"
                            android:src="@drawable/traveling_kiss_icon" />

                        <TextView
                            android:id="@+id/travling_kiss_count_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right|top"
                            android:layout_marginRight="5sp"
                            android:background="@drawable/counter_bg"
                            android:gravity="center"
                            android:textColor="#FFF"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:visibility="gone" />
                    </FrameLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_main_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:orientation="horizontal"
                    android:padding="@dimen/five" >

                    <ImageView
                        android:id="@+id/contacts_kiss"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/five"
                        android:src="@drawable/contact_icon" />

                    <ImageView
                        android:id="@+id/premium_kiss"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/five"
                        android:src="@drawable/premium"
                        android:visibility="gone" />

                    <LinearLayout
                        android:id="@+id/ll_kisses_left"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/five"
                        android:background="@drawable/kisses_left_bg"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/kisses_left_count"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/fifteen"
                            android:text=""
                            android:textColor="@color/white"
                            android:textSize="@dimen/eighteen_sp" />

                        <!-- <TextView
                            android:id="@+id/kisses_left_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:text="@string/kiss_left"
                            android:textColor="@color/white"
                            android:textSize="@dimen/fifteen_sp" /> -->
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</RelativeLayout>