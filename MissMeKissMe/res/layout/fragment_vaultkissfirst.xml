<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/vault_bg" >

    <RelativeLayout
        android:id="@+id/vault_kiss_header"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/black"
        android:padding="5dp" >

        <ImageView
            android:id="@+id/vaultKiss_back_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:clickable="true"
            android:contentDescription="@string/empty_string"
            android:src="@drawable/back_icon" />

        <TextView
            android:id="@+id/vaultKiss_txtsecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:text="@string/vault_kisses"
            android:textColor="@color/white" />
    </RelativeLayout>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true" >

        <ImageView
            android:id="@+id/vaultKiss_code_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:contentDescription="@string/empty_string"
            android:src="@drawable/vault" />

        <RelativeLayout
            android:id="@+id/rr_vault_code"
            android:layout_width="275dp"
            android:layout_height="280dp"
             android:layout_centerHorizontal="true"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/vault_circle_layout"
            android:visibility="gone" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="285dp"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/vault_circle_stroke" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="15dp"
                    android:gravity="center_vertical"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/your"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="5dp"
                        android:padding="5dp"
                        android:text="@string/enter_code"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        android:textStyle="bold" />

                    <com.missme.kissme.customkeyboard.EditTextNoKeyBoard
                        android:id="@+id/sucess"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="5dp"
                        android:background="@drawable/vault_textbox"
                        android:ems="6"
                        android:inputType="textPassword"
                        android:padding="5dp"
                        android:text="@string/empty_string"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <Button
                            android:id="@+id/no_txt1"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/no_txt2"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="1"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/no_txt3"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="2"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/no_txt4"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="3"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/no_txt5"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="4"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/no_txt6"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="5"
                            android:textColor="@color/white" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <Button
                            android:id="@+id/no_txt7"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="6"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/no_txt8"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="7"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/no_txt9"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="8"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/no_txt10"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/vault_no_bg"
                            android:gravity="center"
                            android:text="9"
                            android:textColor="@color/white" />

                        <Button
                            android:id="@+id/arow_txt"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/arrow"
                            android:gravity="center" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/vault_ok_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="6dp"
                        android:contentDescription="@string/empty_string"
                        android:src="@drawable/vault_done_btn" />
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </FrameLayout>
</RelativeLayout>